    "import sqlite3\n",
    "\n",
    "con = sqlite3.connect(\"../chinook.db\")\n",
    "cur = con.cursor()\n",
    "\n",
    "cur.execute(\"\"\"\n",
    "SELECT COUNT(AlbumId), ArtistId\n",
    "FROM albums\n",
    "GROUP BY ArtistId\n",
    "\"\"\")\n",
    "\n",
    "albums = cur.fetchall()\n",
    "\n",
    "for album in albums:\n",
    "    print(album)"
